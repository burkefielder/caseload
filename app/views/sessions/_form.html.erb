<%= form_for([@student, @session]) do |f| %>
  <%= render 'shared/errors', object: @session %>

    <%= f.label :date %>
    <%= f.date_field :date, autofocus: true %>


    <%= f.label :minutes %>
    <%= f.number_field :minutes %>


    <%= f.label :therapy %>
    <%= f.text_field :therapy %>


    <%= f.label :notes %>
    <%= f.text_area :notes, cols: 40, placeholder: 'How did the session go?' %>

  
    <%= f.submit 'Save Note' %>

<% end %>
